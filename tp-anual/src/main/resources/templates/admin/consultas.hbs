{{#partial "contenido"}}

 <form role="form" action="/filtrarConsultas" method="GET">
        <div class="form-group">
            <label class="control-label" for="desde">Consultas desde</label>
            <input class="form-control" i passing multiple objectsd="desde" name="desde" placeholder="DD/MM/YYY" type="text"/>
        </div>
        <div class="form-group">
            <label class="control-label" for="hasta">Consultas hasta</label>
            <input class="form-control" id="hasta" name="hasta" placeholder="DD/MM/YYY" type="text"/>
        </div>
        <div class="form-group">
            <label class="control-label" for="cantidadDevueltos">Cantidad de POIs resultado</label>
            <input class="form-control" id="cantidadDevueltos" name="cantidad" type="text"/>
        </div>
        <div class="form-group" >
	    <label for="terminal">Terminal:</label>
	      <select class="form-control" name="terminal">
		     {{#each terminales}}
			  	<option value={{id}}>{{nombre}}</option>
			  {{/each}}
   		  </select>
	  	</div>
        <button type="submit" class="btn btn-default">Buscar</button>
    </form>
    
{{/partial}}


{{#partial "contenidoScript"}}
<script>
    $(document).ready(function(){
        var desde_input=$('input[name="desde"]');
        var options={
            format: 'dd/mm/yyyy',
            todayHighlight: true,
            autoclose: true,
        };
        desde_input.datepicker(options);
        
        var hasta_input=$('input[name="hasta"]');
        var options={
            format: 'dd/mm/yyyy',
            todayHighlight: true,
            autoclose: true,
        };
        hasta_input.datepicker(options);
    })
</script>
{{/partial}}


